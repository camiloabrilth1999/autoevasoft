<div class="content-header">
  <div class="container-fluid">
    <div class="row mb-2">
      <div class="col-sm-6">
        <h1>Estudiantes</h1>
      </div>
    </div>
  </div>
</div>
<section class="content">
  <div class="container-fluid">
    <div class="row">
      <div class="col-lg-12">
        <div class="card">
          <!-- card-header -->
          <div class="card-header">
            <h3 class="card-title">Seleccione los estudiantes por semestre</h3>
          </div>
          <!-- /.card-header -->
          <!-- card-body -->
          <div class="card-body">
            <div id="example2_wrapper" class="dataTables_wrapper dt-bootstrap4">
              <div class="row">
                <div class="col-sm-12 col-md-12">
                  <p>Semestre:</p>
                  <%= form_tag index_semester_post_self_evaluations_sampling_instruments_students_path, multipart: true, class: "" do %>
                    <%=select_tag "semesters",
                    raw("<option>PRIMER SEMESTRE</option><option>SEGUNDO SEMESTRE</option>
                      <option>TERCER SEMESTRE</option><option>CUARTO SEMESTRE</option>
                      <option>QUINTO SEMESTRE</option><option>SEXTO SEMESTRE</option>
                      <option>SEPTIMO SEMESTRE</option><option>OCTAVO SEMESTRE</option>
                      <option>NOVENO SEMESTRE</option><option>DECIMO SEMESTRE</option>"),
                    class: "col-md-12 select_semester"
                    %>
                    <%= button_tag(type: "submit", class: "btn btn-custom float-right") do %>
                      <i class="fas fa-search card_text_white"></i>
                      Buscar
                    <% end %>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
          <!-- /.card-body -->
        </div>

        <div class="card">
          <!-- card-body -->
          <div class="card-body">
            <div id="example2_wrapper" class="dataTables_wrapper dt-bootstrap4">
              <div class="row">
                <div class="col-sm-12 col-md-12">
                  <%= form_tag import_self_evaluations_sampling_instruments_students_path, multipart: true, class: "float-right" do %>
                    <%= file_field_tag :file, class: "" %>
                    <%= button_tag(type: "submit", class: "btn btn-custom-2") do %>
                        <i class="fas fa-upload card_text_white"></i>
                        Cargar estudiantes
                    <% end %>
                  <% end %>
                </div>
              </div>
            </div>
          </div>
          <!-- /.card-body -->
        </div>
      </div>
    </div>
  </div>
</section>
